{
	"productName": "mcge-daily-details",
	"version": "0.0.1",
	"identifier": "club.gemc.daily",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"devUrl": "http://localhost:9428",
		"beforeBuildCommand": "pnpm build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"label": "main",
				"title": "Daily Details",
				"x": 100,
				"y": 40,
				"width": 1200,
				"height": 800,
				"fullscreen": false,
				"maximizable": false,
				"resizable": false,
				"decorations": true,
				"visible": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
